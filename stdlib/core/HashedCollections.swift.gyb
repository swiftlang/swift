    // FIXME: this cast is invalid.
    let buffer = ${Self}HeapBuffer(
      unsafeBitCast(self, HeapBufferStorage.self))
    let body = buffer.value
    buffer._value.destroy()
    buffer.baseAddress.destroy(body.capacity)
