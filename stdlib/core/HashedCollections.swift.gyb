    var position = 0 // position in the dictionary storage
    let count = bridgedNativeStorage.capacity
    let unmanagedKeys = _UnmanagedAnyObjectArray(keys)
    let unmanagedObjects = _UnmanagedAnyObjectArray(objects)

    if keys == nil {
