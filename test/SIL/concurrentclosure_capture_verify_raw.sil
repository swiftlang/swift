@_hasMissingDesignatedInitializers
class F {
  func useConcurrent(_: @escaping @Sendable () -> ())
  init()
}

enum FakeOptional<T> {
case none
case some(T)
}

struct Int {
  var _value: Builtin.Int64
}

class Klass {}

