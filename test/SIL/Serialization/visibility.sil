// RUN: %target-swift-frontend %s -parse-sil -sil-serialize-all -emit-module -o - -module-name Swift -module-link-name swiftCore -parse-as-library -parse-stdlib | %target-sil-opt -verify -module-name Swift | FileCheck %s
// RUN: %target-swift-frontend %s -parse-sil -sil-serialize-all -emit-module -o - -module-name Swift -module-link-name swiftCore -parse-as-library -parse-stdlib | %target-sil-opt -verify -module-name Swift | FileCheck --check-prefix=NEG-CHECK %s

import Builtin

// To prevent issues with the SILPrinter ordering things we separate our
// positive/negative checks to simplify things.

// NEG-CHECK-NOT: sil_global private @private_global : $Builtin.Word
// NEG-CHECK-NOT: sil private @private_function : $@convention(thin) () -> ()
// NEG-CHECK-NOT: sil @references_private_global : $@convention(thin) () -> () {
// NEG-CHECK-NOT: sil @references_private_function : $@convention(thin) () -> () {
// NEG-CHECK-NOT: sil private @private_function_test : $@convention(thin) () -> ()

// Check Global Variables.
// CHECK: sil_global shared [fragile] @shared_global : $Builtin.Word
// CHECK: sil_global [fragile] @public_global : $Builtin.Word
// CHECK: sil_global hidden [fragile] @hidden_global : $Builtin.Word

// Check that all functions are defined/declared appropriately.
// CHECK-NOT: sil [fragile] @public_external_function_test : $@convention(thin) () -> () {
// CHECK: sil [fragile] @public_external_function_test : $@convention(thin) () -> ()
// CHECK: sil hidden [fragile] @hidden_function_test : $@convention(thin) () -> () {
// CHECK: sil [fragile] @references_public_function : $@convention(thin) () -> () {
// CHECK: sil [fragile] @references_shared_function : $@convention(thin) () -> () {
// CHECK: sil [fragile] @references_hidden_function : $@convention(thin) () -> () {
// CHECK: sil [fragile] @references_private_function : $@convention(thin) () -> ()
// CHECK: sil [fragile] @references_public_global : $@convention(thin) () -> () {
// CHECK: sil [fragile] @references_shared_global : $@convention(thin) () -> () {
// CHECK: sil [fragile] @references_hidden_global : $@convention(thin) () -> () {
// CHECK: sil [fragile] @references_private_global : $@convention(thin) () -> ()
// CHECK: sil hidden_external [fragile] @hidden_external_function_test : $@convention(thin) () -> ()
// CHECK: sil [fragile] @public_function_test : $@convention(thin) () -> () {
// CHECK: sil [fragile] @public_function : $@convention(thin) () -> () {
// CHECK: sil hidden [fragile] @hidden_function : $@convention(thin) () -> () {
// CHECK-NOT: sil shared_external [fragile] @shared_external_function_test : $@convention(thin) () -> ()
// CHECK: sil shared [fragile] @shared_external_function_test : $@convention(thin) () -> ()

sil_global private [fragile] @private_global : $Builtin.Word
sil_global [fragile] @public_global : $Builtin.Word
sil_global shared [fragile] @shared_global : $Builtin.Word
sil_global hidden [fragile] @hidden_global : $Builtin.Word

sil private [fragile] @private_function : $@convention(thin) () -> () {
  %0 = integer_literal $Builtin.Int32, 0
  %1 = tuple()
  return %1 : $()
}
sil [fragile] @references_private_global : $@convention(thin) () -> () {
  %0 = global_addr @private_global : $*Builtin.Word
  %1 = tuple()
  return %1 : $()
}
sil [fragile] @references_private_function : $@convention(thin) () -> () {
  %0 = function_ref @private_function : $@convention(thin) () -> ()
  %1 = tuple()
  return %1 : $()
}

sil shared [fragile] @shared_function : $@convention(thin) () -> () {
  %0 = integer_literal $Builtin.Int32, 1
  %1 = tuple()
  return %1 : $()
}
sil [fragile] @references_shared_global : $@convention(thin) () -> () {
  %0 = global_addr @shared_global : $*Builtin.Word
  %1 = tuple()
  return %1 : $()
}
sil [fragile] @references_shared_function : $@convention(thin) () -> () {
  %0 = function_ref @shared_function : $@convention(thin) () -> ()
  %1 = tuple()
  return %1 : $()
}

sil hidden [fragile] @hidden_function : $@convention(thin) () -> () {
  %0 = integer_literal $Builtin.Int32, 2
  %1 = tuple()
  return %1 : $()
}
sil [fragile] @references_hidden_global : $@convention(thin) () -> () {
  %0 = global_addr @hidden_global : $*Builtin.Word
