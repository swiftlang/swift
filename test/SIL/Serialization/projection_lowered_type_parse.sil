// RUN: %target-swift-frontend -emit-module %s -module-name Swift -sil-serialize-all -module-link-name swiftCore -parse-as-library -parse-sil -parse-stdlib -o - | %target-sil-opt -verify -module-name=Swift

struct A {
  var f : () -> ()
}

sil @callee_check : $@thin (@callee_owned () -> ()) -> ()

sil @test1 : $@thin (@inout A) -> () {
bb0(%0 : $*A):
  %1 = struct_element_addr %0 : $*A, #A.f
  %2 = load %0 : $*A
  %3 = struct_extract %2 : $A, #A.f
  %4 = function_ref @callee_check : $@thin (@callee_owned () -> ()) -> ()
  apply %4(%3) : $@thin (@callee_owned () -> ()) -> ()
  %5 = tuple()
  return %5 : $()
}

class B {
  var f : () -> ()
  init()
}

sil @test2 : $@thin (B) -> () {
bb0(%0 : $B):
  %1 = ref_element_addr %0 : $B, #B.f
  %2 = tuple()
  return %2 : $()
}
