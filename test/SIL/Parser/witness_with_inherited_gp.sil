import Swift

public struct mmEnumerateGenerator<
  Base: GeneratorType
> : GeneratorType, Sequence {
  public typealias Element = (index: Int, element: Base.Element)
  var base: Base
  var count: Int

  public init(_ base: Base)
  public mutating func next() -> Element?
  public typealias Generator = mmEnumerateGenerator<Base>
  public func makeIterator() -> Generator
}

public struct mmEnumerateSequence<
  Base: Sequence
> : Sequence {
  var base: Base
  public init(_ base: Base)
  public func makeIterator() -> mmEnumerateGenerator<Base.Generator>
}

