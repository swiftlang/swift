// CHECK-SAME:  [[INT]] 0,
// CHECK-SAME:  [[INT]] 4 }

sil @test0 : $() -> () {
bb0:
  %0 = metatype $@thick HasNestedUnion.Type

  %ret = tuple ()
  return %ret : $()
}

