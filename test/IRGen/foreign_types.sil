// CHECK-SAME:  i8* getelementptr inbounds
// CHECK-SAME:  %swift.type* null,
// CHECK-SAME:  [[INT:i[0-9]+]] 1,
// CHECK-SAME:  @_T0SC14HasNestedUnionV18__Unnamed_struct_sVWV
