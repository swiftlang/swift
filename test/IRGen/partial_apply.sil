sil @objc_partial_apply : $@thin ObjCClass -> @callee_owned Int64 -> () {
entry(%c : $ObjCClass):
  %m = class_method [volatile] %c : $ObjCClass, #ObjCClass.method!1.foreign : $@cc(objc_method) @thin (Int64, ObjCClass) -> ()
  %p = partial_apply %m(%c) : $@cc(objc_method) @thin (Int64, ObjCClass) -> ()
  return %p : $@callee_owned Int64 -> ()
}

// CHECK-LABEL: define { i8*, %swift.refcounted* } @objc_partial_apply_consumes_self(%CSo9ObjCClass*) {
