sil @indirect_partial_apply : $@thin ((Builtin.Int64) -> (), Builtin.Int64) -> @callee_owned () -> () {
entry(%f : $(Builtin.Int64) -> (), %x : $Builtin.Int64):
  %p = partial_apply %f(%x) : $(Builtin.Int64) -> ()
  return %p : $@callee_owned () -> ()
}

