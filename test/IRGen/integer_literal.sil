sil @trunc_s32 : $(Builtin.IntLiteral) -> (Builtin.Int32, Builtin.Int1) {
entry(%0 : $Builtin.IntLiteral):
  // CHECK:         [[WIDTH:%.*]] = lshr i32 %1, 8
