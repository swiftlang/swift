sil_vtable GenericInheritsObjC2 {}

// __deallocating_deinit
sil @$s20generic_classes_objc20GenericInheritsObjC2CfD : $@convention(method) <T> (GenericInheritsObjC2<T>) -> () {
bb0(%0 : @unowned $GenericInheritsObjC2<T>):
