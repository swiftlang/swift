//     apply undef<T>(%first_out_addr)
//     %enum_data_addr = init_enum_data_addr %out_addr
//     %second_out_addr = tuple_element_addr %enum_data_addr, 1
//     copy_addr %second_in_addr to [init] %second_out_addr
