sil [ossa] @simple_forwarding4 : $@convention(thin) (Builtin.Int1) -> () {
bb0(%0 : $Builtin.Int1):
  cond_br %0, bb1, bb2

bb1:
  %2 = enum $PrimaryColor, #PrimaryColor.red!enumelt
  br bb3(%2 : $PrimaryColor)

bb2:
  %3 = enum $PrimaryColor, #PrimaryColor.blue!enumelt
  br bb3(%3 : $PrimaryColor)

