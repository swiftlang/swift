  %9 = function_ref @callee : $@convention(thin) () -> ()
  %10 = apply %9() : $@convention(thin) () -> ()
  switch_enum %8, case #E.A!enumelt: bb4, case #E.B!enumelt: bb5

bb4(%12 : @guaranteed $Klass):
  br bb6

bb5(%14 : @guaranteed $Klass):
  br bb6

bb6:
  %16 = tuple ()
  return %16
}

