  switch_enum %14 : $PrimaryColor, case #PrimaryColor.red!enumelt: bb4, default bb5

bb4:
  br bb6

bb5(%18 : $PrimaryColor):
  %15 = function_ref @use_color : $@convention(thin) (PrimaryColor) -> ()
  %16 = apply %15(%18) : $@convention(thin) (PrimaryColor) -> ()
