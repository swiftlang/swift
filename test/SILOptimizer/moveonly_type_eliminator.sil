struct Trivial {
  var value: Builtin.Int32
}

enum FakeOptional<T> {
case none
case some(T)
}

struct TrivialPair {
  var lhs: Trivial
  var rhs: Trivial
}

sil @use_trivial : $@convention(thin) (Trivial) -> ()

sil @readIndirectTrivial : $@convention(method) (@in_guaranteed Trivial) -> ()

class Klass {
  func foo()
}

class C {}

struct KlassPair {
  var lhs: Klass
  var rhs: Klass
}

sil @use_klass : $@convention(thin) (@guaranteed Klass) -> ()

