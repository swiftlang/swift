func getPositiveSubnormal_Float32() -> Float32 {
  var result: Float32 = 1.0
  for i in 0..<127 {
    result /= 2.0 as Float32
  }
  expectTrue(result.isSubnormal)
  return result
}

func getPositiveSubnormal_Float64() -> Float64 {
  var result: Float64 = 1.0
  for i in 0..<1023 {
    result /= 2.0 as Float64
  }
  expectTrue(result.isSubnormal)
  return result
}

